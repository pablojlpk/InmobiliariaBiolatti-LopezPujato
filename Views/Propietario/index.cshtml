@model IEnumerable<Propietario>
@{
    ViewData["Title"] = "Propietario";
}
<h1>@ViewData["Title"]</h1>

<p>LISTA PROPIETARIOS.</p>


<div class="row">
    <div class="col-md-8">
        <table class="table">
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Teléfono</th>
                    <th>DNI</th>
                    <th>Email</th>
                    <th>Dirección</th>
                    <th>Acciones</th>
                </tr>
                <td>
                    @foreach (var item in Model)
                    {
                        <tr>
                            
                            <td>@item.nombre</td>
                            <td>@item.apellido</td>
                            <td>@item.dni</td>
                            <td>@item.clave</td>
                            
                        </tr>
                    }
                </td>
                <br>

            </thead>

        </table>
    </div>
</div>

<a asp-action="Create" class="btn btn-success">Crear nuevo</a>
<i class="fa-solid fa-pen-to-square"></i> <a asp-action="Editar">Editar</a> |
<i class="fa-solid fa-trash"></i> <a asp-action="Eliminar">Eliminar</a>
